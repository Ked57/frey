{
  "name": "frey",
  "version": "1.0.0",
  "description": "Entity-driven API generation framework for Fastify",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "type": "module",
  "files": [
    "dist/src/**/*",
    "dist/package.json",
    "dist/README.md",
    "dist/LICENSE"
  ],
  "scripts": {
    "dev:nowatch": "node --loader ts-node/esm ./example.ts",
    "dev": "nodemon",
    "build": "tsc --outDir dist --declaration --declarationMap && cp ../../README.md dist/ && cp ../../LICENSE dist/",
    "build:dist": "tsc --outDir dist --declaration --declarationMap && cp ../../README.md dist/ && cp ../../LICENSE dist/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"example.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"example.ts\"",
    "type-check": "tsc --noEmit --strict",
    "audit": "npm audit --audit-level=moderate",
    "semantic-release": "semantic-release",
    "clean": "rm -rf dist"
  },
  "keywords": ["fastify", "api", "framework", "entity", "crud"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "24.5.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "audit-ci": "7.1.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "fastify": "5.6.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "semantic-release": "^24.2.8",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "zod": "4.1.8"
  }
}
